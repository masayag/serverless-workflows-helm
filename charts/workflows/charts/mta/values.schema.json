{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
      "mta",
      "labels"
  ],
  "properties": {
      "mta": {
          "type": "object",
          "default": {},
          "title": "The mta Schema",
          "required": [
              "endpoint"
          ],
          "properties": {
              "endpoint": {
                  "type": "string",
                  "default": "",
                  "title": "The endpoint Schema",
                  "examples": [
                      "https://mta-openshift-mta.apps.parodos-dev.projects.ecosystem.sysdeseng.com:443/hub/"
                  ]
              }
          },
          "examples": [{
              "endpoint": "https://mta-openshift-mta.apps.parodos-dev.projects.ecosystem.sysdeseng.com:443/hub/"
          }]
      },
      "labels": {
          "type": "object",
          "default": {},
          "title": "The labels Schema",
          "required": [
              "app"
          ],
          "properties": {
              "app": {
                  "type": "string",
                  "default": "",
                  "title": "The app Schema",
                  "examples": [
                      "mta"
                  ]
              }
          },
          "examples": [{
              "app": "mta"
          }]
      }
  },
  "examples": [{
      "mta": {
          "endpoint": "https://mta-openshift-mta.apps.parodos-dev.projects.ecosystem.sysdeseng.com:443/hub/"
      },
      "labels": {
          "app": "mta"
      }
  }]
}